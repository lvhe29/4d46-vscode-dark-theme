    {
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#ababab",
        "background": "#0b0b0b"
      }
    },
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#505050",
        "background": "#0b0b0b",
        "fontStyle": "italic"
      }
    },
    {
