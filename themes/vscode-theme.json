        "foreground": "#6d6d6d",
        "background": "#0b0b0b",
        "fontStyle": "none"
      }
    },
    {
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#d7d7d7",
        "background": "#0b0b0b",
        "fontStyle": "bold"
